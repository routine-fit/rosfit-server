{
  "name": "typescript-server-template",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "nodemon",
    "start:withEmulators": "export FIREBASE_AUTH_EMULATOR_HOST=\"127.0.0.1:9099\" nodemon",
    "start:build": "node dist/index.js",
    "set:emulators": "firebase emulators:start",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"./**/*.{ts,test.ts}\"",
    "format": "prettier --write './**/*.{ts,test.ts}' --config ./.prettierrc",
    "lint:fix": "eslint --fix && yarn format",
    "pre-commit": "yarn type-check && yarn lint",
    "prepare": "husky install",
    "set:local": "docker-compose up -d && prisma db push",
    "seed": "ts-node --transpile-only prisma/seed/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "5.5.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "firebase-admin": "^11.11.0",
    "firebase-tools": "^12.9.1",
    "prisma": "^5.5.2",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.2.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^5.2.2"
  }
}
